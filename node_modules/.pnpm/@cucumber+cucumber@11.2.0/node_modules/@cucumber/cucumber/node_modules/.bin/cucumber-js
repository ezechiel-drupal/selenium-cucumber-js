#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/@cucumber+cucumber@11.2.0/node_modules/@cucumber/cucumber/bin/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/@cucumber+cucumber@11.2.0/node_modules/@cucumber/cucumber/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/@cucumber+cucumber@11.2.0/node_modules/@cucumber/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/@cucumber+cucumber@11.2.0/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/@cucumber+cucumber@11.2.0/node_modules/@cucumber/cucumber/bin/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/@cucumber+cucumber@11.2.0/node_modules/@cucumber/cucumber/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/@cucumber+cucumber@11.2.0/node_modules/@cucumber/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/@cucumber+cucumber@11.2.0/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cucumber.js" "$@"
else
  exec node  "$basedir/../../bin/cucumber.js" "$@"
fi
