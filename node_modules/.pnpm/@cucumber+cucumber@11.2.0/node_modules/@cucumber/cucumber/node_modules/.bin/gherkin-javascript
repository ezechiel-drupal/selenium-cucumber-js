  exec node  "$basedir/../../../../../../@cucumber+gherkin-streams@5.0.1_@cucumber+gherkin@30.0.4_@cucumber+message-streams@4.0.1_@cuc_unruhbulpngcittl44rukez6oq/node_modules/@cucumber/gherkin-streams/bin/gherkin" "$@"
