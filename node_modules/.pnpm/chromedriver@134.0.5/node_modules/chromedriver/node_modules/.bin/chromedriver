#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/chromedriver@134.0.5/node_modules/chromedriver/bin/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/chromedriver@134.0.5/node_modules/chromedriver/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/chromedriver@134.0.5/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/chromedriver@134.0.5/node_modules/chromedriver/bin/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/chromedriver@134.0.5/node_modules/chromedriver/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/chromedriver@134.0.5/node_modules:/Users/odinasgard/Desktop/selenium-cucumber-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/chromedriver" "$@"
else
  exec node  "$basedir/../../bin/chromedriver" "$@"
fi
